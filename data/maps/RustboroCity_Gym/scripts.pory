script RustboroCity_Gym_CheckBadge {
	if(flag(FLAG_DEFEATED_RUSTBORO_GYM)) {
		end
	}
	else {
		call(RustboroCity_Gym_ChallengeSierra)
	}
}

script RustboroCity_Gym_ChallengeSierra {
	lock
	special(SpawnCameraObject)
	special(HealPlayerParty)
	applymovement(LOCALID_CAMERA, moves(
		walk_up * 2
	))
	waitmovement(0)
	getpartysize
	delay(20)
	msgbox(format(
		"SIERRA: I knew I'd see you again.\p"
		"I'm Sierra, the gym leader of Rustboro City.\p"
		"If you can win against me, you'll earn the Shutter Badge and be on the path to the POKéMON CHAMPIONSHIP."
	))
	waitmessage
	closemessage
	if(var(VAR_RESULT) < 2) {
		msgbox(format("Oh… I'm sorry, all official gym challenges are double battles. You don't have enough POKéMON for that right now."))
		waitmessage
		applymovement(LOCALID_CAMERA, moves(
			walk_down * 2
		))
		waitmovement(0)
		special(RemoveCameraObject)
		release
	}
	else {
	applymovement(LOCALID_PLAYER, moves(
		walk_left * 4
		walk_up * 2
		walk_in_place_fast_right
	))
	applymovement(1, moves(
		walk_right * 4
		walk_down * 2
		walk_in_place_fast_left
	))
	waitmovement(0)
	special(RemoveCameraObject)
	msgbox(format("Let's see if you can win against my shockingly cute POKéMON!"))
	trainerbattle_no_intro(
	// trainerbattle_double(
		TRAINER_SIERRA, 
		// format("Let's see if you can win against my shockingly cute POKéMON!"), 
		format(
			"Wow! You did it! By defeating a gym leader, you earn a gym badge!\p"
			"Please accept the official POKéMON\n"
			"LEAGUE Shutter Badge."), 
		// format("N/A"), 
		// RustboroCity_Gym_SierraDefeated, 
		// NO_MUSIC
	)
	goto(RustboroCity_Gym_SierraDefeated)
	}
}

script RustboroCity_Gym_SierraDefeated {
	special(SpawnCameraObject)
	message(format("{PLAYER} received the Shutter Badge from Sierra."))
	waitmessage
	call(Common_EventScript_PlayGymBadgeFanfare)
	applymovement(1, moves(
		walk_left * 3
	))
	applymovement(LOCALID_PLAYER, moves(
		walk_right * 3
		unlock_facing_direction
	))
	waitmovement(0)
	applymovement(LOCALID_CAMERA, moves(
		walk_left
	))
	waitmovement(0)
	special(RemoveCameraObject)
	msgbox(format("That badge gives you the ability to use the HM move, FLASH, outside of battle.\p""Also, defeated gym leaders give successful challengers a TM to help them on their journeys. Here…"))
	setflag(FLAG_DEFEATED_RUSTBORO_GYM)
	setflag(FLAG_BADGE01_GET)
	setvar(VAR_RUSTBORO_CITY_STATE, 1)
	addvar(VAR_PETALBURG_GYM_STATE, 1)
	setvar(VAR_0x8008, 1)
	call(Common_EventScript_SetGymTrainers)
	call_if_eq(VAR_PETALBURG_GYM_STATE, 6, Common_EventScript_ReadyPetalburgGymForBattle)
	goto(RustboroCity_Gym_GiveTM)
}

script RustboroCity_Gym_GiveTM {
	giveitem(ITEM_TM_ROCK_TOMB)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	setflag(FLAG_RECEIVED_TM_ROCK_TOMB)
	msgbox(format("That TECHNICAL MACHINE, TM 39, contains CHARGE BEAM.\p""It not only inflicts damage, it can also raise Sp. Atk.\p""If you use a TM, it instantly teaches the move to a POKéMON."))
	release
}

script RustboroCity_Gym_Sierra {
	lock
	trainerbattle_double(TRAINER_SIERRA, RustboroCity_Gym_Text_RoxanneIntro, RustboroCity_Gym_Text_RoxanneDefeat, RustboroCity_Gym_Text_RoxanneRematchNeedTwoMons, RustboroCity_Gym_SierraDefeated, NO_MUSIC)
	msgbox(RustboroCity_Gym_Text_RoxannePostBattle)
	release
}

raw `
RustboroCity_Gym_EventScript_Roxanne::
	trainerbattle_single TRAINER_SIERRA, RustboroCity_Gym_Text_RoxanneIntro, RustboroCity_Gym_Text_RoxanneDefeat, RustboroCity_Gym_EventScript_RoxanneDefeated, NO_MUSIC
	specialvar VAR_RESULT, ShouldTryRematchBattle
	goto_if_eq VAR_RESULT, TRUE, RustboroCity_Gym_EventScript_RoxanneRematch
	goto_if_unset FLAG_RECEIVED_TM_ROCK_TOMB, RustboroCity_Gym_EventScript_GiveRockTomb
	msgbox RustboroCity_Gym_Text_RoxannePostBattle, MSGBOX_DEFAULT
	release
	end

RustboroCity_Gym_EventScript_RoxanneDefeated::
	message RustboroCity_Gym_Text_ReceivedStoneBadge
	waitmessage
	call Common_EventScript_PlayGymBadgeFanfare
	msgbox RustboroCity_Gym_Text_StoneBadgeInfoTakeThis, MSGBOX_DEFAULT
	setflag FLAG_DEFEATED_RUSTBORO_GYM
	setflag FLAG_BADGE01_GET
	setvar VAR_RUSTBORO_CITY_STATE, 1
	addvar VAR_PETALBURG_GYM_STATE, 1
	setvar VAR_0x8008, 1
	call Common_EventScript_SetGymTrainers
	call_if_eq VAR_PETALBURG_GYM_STATE, 6, Common_EventScript_ReadyPetalburgGymForBattle
	goto RustboroCity_Gym_EventScript_GiveRockTomb
	end

RustboroCity_Gym_EventScript_GiveRockTomb::
	giveitem ITEM_TM_ROCK_TOMB
	goto_if_eq VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull
	setflag FLAG_RECEIVED_TM_ROCK_TOMB
	msgbox RustboroCity_Gym_Text_ExplainRockTomb, MSGBOX_DEFAULT
	release
	end

