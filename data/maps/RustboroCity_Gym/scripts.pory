script RustboroCity_Gym_EventScript_PressFloorSwitch {
	special(MauvilleGymSetDefaultBarriers)
	special(MauvilleGymPressSwitch)
	special(DrawWholeMapView)
	playse(SE_UNLOCK)
	goto_if_set(FLAG_RUSTBORO_GYM_BARRIERS_STATE, RustboroCity_Gym_EventScript_ClearBarriersAltState)
	goto_if_unset(FLAG_RUSTBORO_GYM_BARRIERS_STATE, RustboroCity_Gym_EventScript_SetBarriersAltState)
	releaseall
}

script RustboroCity_Gym_EventScript_SwitchDoNothing {
	releaseall
}

script RustboroCity_Gym_EventScript_SetBarriersAltState {
	setflag(FLAG_RUSTBORO_GYM_BARRIERS_STATE)
	releaseall
}

script RustboroCity_Gym_EventScript_ClearBarriersAltState {
