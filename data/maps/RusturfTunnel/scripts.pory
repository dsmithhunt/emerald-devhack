script RusturfTunnel_AquaGruntsChat{
	lockall
	setflag(FLAG_SAFE_FOLLOWER_MOVEMENT)
	msgbox(format(
		"MALE GRUNT: Great, you got the parts! I got a hostage."
		))
	playse(SE_BOO)
	applymovement(6, moves(
		emote_question_mark
	))
	msgbox(format(
		"FEMALE GRUNT: What do you mean you got a hostage?!\p"
		"MALE GRUNT: Some geezer had this Wingull at the park. I grabbed it\l""so no one would chase us.\p"
		"FEMALE GRUNT: What if they chase us because you took it? These are just parts; that's someone's POKéMON!"
		))
	playse(SE_PIN)
	applymovement(11, moves(
		emote_exclamation_mark
		face_left
	))
	applymovement(6, moves(
		face_left
	))
	waitmovement(0)
	msgbox(format(
		"MALE GRUNT: Uh oh, someone's coming!\n"
		"Shelly said Rustboro was wide open…\p"
		"Wait, it's just some kid.\p"
		"Hey, kid, stay back! I'm warning you…"
		))
	applymovement(6, RusturfTunnel_Movement_GruntAndPeekoBackUp)
	applymovement(7, RusturfTunnel_Movement_GruntAndPeekoBackUp)
	applymovement(11, RusturfTunnel_Movement_GruntAndPeekoBackUp)
	waitmovement(0)
	copyobjectxytoperm(6)
	copyobjectxytoperm(7)
	copyobjectxytoperm(11)
	setvar(VAR_RUSTURF_TUNNEL_STATE, 6)
	releaseall
}

script RusturfTunnel_AquaTrigger0 {
	setvar(VAR_TEMP_1, 0)
	call(RusturfTunnel_SierraArrive)
}

script RusturfTunnel_AquaTrigger1 {
	setvar(VAR_TEMP_1, 1)
	call(RusturfTunnel_SierraArrive)
}

script RusturfTunnel_SierraArrive {
	lockall
	playbgm(MUS_ENCOUNTER_AQUA, TRUE)
	applymovement(6, moves(walk_in_place_fast_left))
	applymovement(11, moves(walk_in_place_fast_left))
	if (checkplayergender() == MALE){
		msgbox(format("MALE GRUNT: We warned you not to stick your nose where it doesn't belong!\p""Come on, let's get him!"))
	} else {
		msgbox(format("MALE GRUNT: We warned you not to stick your nose where it doesn't belong!\p""Come on, let's get her!"))
	}
	msgbox(format("???: Stop!"))
	addobject(12)
	applymovement(12, moves(
		walk_fast_right*7
	))
	waitmovement(0)
	if (var(VAR_TEMP_1) == 0){
	applymovement(LOCALID_PLAYER, moves(
		walk_fast_down
		face_right
	))
	}
	applymovement(12, moves(
		walk_right*2
	))
	waitmovement(0)
	msgbox(format(
		"SIERRA: I don't want to have to deal with this right now.\p""I go visit my brother for one day and some criminals attack the city?"
	))
	applymovement(12, moves(
		face_player
	))
	applymovement(LOCALID_PLAYER, moves(
		face_up
	))
	waitmovement(0)
	msgbox(format(
		"SIERRA: Hey, you, help me with these two."
	))
	call(RusturfTunnel_PartySelect)
}

script RusturfTunnel_PartySelect{
	special(SavePlayerParty)
	fadescreen(FADE_TO_BLACK)
	special(ChooseHalfPartyForBattle)
	waitstate
	goto_if_ne(VAR_RESULT, 0, RusturfTunnel_GruntsBattle)
	special(LoadPlayerParty)
	msgbox(format("SIERRA: This is no time to doubt yourself. Ready your POKéMON!"))
	call(RusturfTunnel_PartySelect)
}

script RusturfTunnel_GruntsBattle{
	applymovement(LOCALID_PLAYER, moves(
		face_right
	))
	applymovement(12, moves(
		face_right
	))
	msgbox(format(
		"MALE GRUNT: Grah, keelhaul it all!\n""That hostage POKéMON was worthless!\p"
		"FEMALE GRUNT: I can't believe you decided we'd hide in this dead-end cave.\p"
		"MALE GRUNT: Nothing to do now but fight our way out!"
	))
	playbgm(MUS_PETALBURG_WOODS, TRUE)
	multi_2_vs_2(TRAINER_GRUNT_WEATHER_INST_4, RusturfTunnel_Text_GruntDefeat, TRAINER_GRUNT_RUSTURF_TUNNEL, RusturfTunnel_Text_GruntDefeat, PARTNER_SIERRA)
	msgbox(RusturfTunnel_Text_GruntTakePackage)
	giveitem(ITEM_DEVON_PARTS)
	closemessage
	applymovement(12, moves(
		lock_facing_direction
		walk_fast_left
		unlock_facing_direction
		walk_in_place_fast_left
	))
	applymovement(LOCALID_PLAYER, RusturfTunnel_Movement_PushPlayerAsideForGrunt)
	applymovement(LOCALID_RUSTURF_TUNNEL_GRUNT, moves(walk_fast_left))
	applymovement(11, moves(walk_fast_down))
	waitmovement(0)
	applymovement(LOCALID_RUSTURF_TUNNEL_GRUNT, RusturfTunnel_Movement_GruntEscape)
	applymovement(11, RusturfTunnel_Movement_GruntEscape)
	waitmovement(0)
	removeobject(LOCALID_RUSTURF_TUNNEL_GRUNT)
	removeobject(11)
	applymovement(12, moves(walk_in_place_fast_right))
	msgbox(format(
		"SIERRA: Sorry you got caught up in that, but you seem like a good trainer. You should challenge the Rustboro Gym.\p"
		"I'll see you around."
		))
	closemessage
	applymovement(12, moves(
		walk_down
		walk_left * 7))
	waitmovement(0)
	removeobject(12)
	delay(50)
	addobject(LOCALID_RUSTURF_TUNNEL_BRINEY)
	applymovement(LOCALID_RUSTURF_TUNNEL_BRINEY, RusturfTunnel_Movement_BrineyApproachPeeko1)
	waitmovement(0)
	applymovement(LOCALID_PLAYER, RusturfTunnel_Movement_PlayerMoveAsideForBriney)
	applymovement(LOCALID_RUSTURF_TUNNEL_PEEKO, moves(walk_left))
	applymovement(LOCALID_RUSTURF_TUNNEL_BRINEY, RusturfTunnel_Movement_BrineyApproachPeeko2)
	waitmovement(0)
	msgbox(RusturfTunnel_Text_PeekoGladToSeeYouSafe)
	applymovement(LOCALID_RUSTURF_TUNNEL_BRINEY, Common_Movement_FacePlayer)
	waitmovement(0)
	message(RusturfTunnel_Text_ThankYouLetsGoHomePeeko)
	waitmessage
	waitse
	playmoncry(SPECIES_WINGULL, CRY_MODE_NORMAL)
	waitbuttonpress
	waitmoncry
	closemessage
	applymovement(LOCALID_PLAYER, RusturfTunnel_Movement_PlayerWatchBrineyExit)
	applymovement(LOCALID_RUSTURF_TUNNEL_BRINEY, RusturfTunnel_Movement_BrineyExit)
	applymovement(LOCALID_RUSTURF_TUNNEL_PEEKO, RusturfTunnel_Movement_PeekoExit)
	waitmovement(0)
	removeobject(LOCALID_RUSTURF_TUNNEL_BRINEY)
	removeobject(LOCALID_RUSTURF_TUNNEL_PEEKO)
	delay(50)
	clearflag(FLAG_DEVON_GOODS_STOLEN)
	setflag(FLAG_RECOVERED_DEVON_GOODS)
	setvar(VAR_RUSTURF_TUNNEL_STATE, 3)
	setvar(VAR_RUSTBORO_CITY_STATE, 4)
	setvar(VAR_BRINEY_HOUSE_STATE, 1)
	setflag(FLAG_HIDE_ROUTE_116_MR_BRINEY)
	releaseall
}

raw `
RusturfTunnel_MapScripts::
	map_script MAP_SCRIPT_ON_TRANSITION, RusturfTunnel_OnTransition
	map_script MAP_SCRIPT_ON_FRAME_TABLE, RusturfTunnel_OnFrame
	.byte 0

RusturfTunnel_OnFrame:
	map_script_2 VAR_RUSTURF_TUNNEL_STATE, 4, RusturfTunnel_EventScript_ClearTunnelScene
	map_script_2 VAR_RUSTURF_TUNNEL_STATE, 5, RusturfTunnel_EventScript_ClearTunnelScene
	.2byte 0

RusturfTunnel_OnTransition:
	call_if_eq VAR_RUSTURF_TUNNEL_STATE, 2, RusturfTunnel_EventScript_SetAquaGruntAndPeekoPos
	end

RusturfTunnel_EventScript_SetAquaGruntAndPeekoPos::
	setobjectxyperm LOCALID_RUSTURF_TUNNEL_PEEKO, 14, 4
	setobjectxyperm LOCALID_RUSTURF_TUNNEL_GRUNT, 13, 5
	setobjectxyperm 11, 13, 4
	return

